int ControlState = 0;
void Controls(){
  Brect(1,1,318,318);
  fill(TextColor);
  textSize(16);
  text("Controles",160-textWidth("Controles")/2,30);
  line(160-textWidth("Controles")/2,32,160+textWidth("Controles")/2,32);
  textSize(12);
  if(ControlState >= 0){ 
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/arrow-left-icon.png",20,80,40,40);
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/arrow-down-icon.png",60,80,40,40);
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/arrow-right-icon.png",100,80,40,40);
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/arrow-up-icon.png",60,40,40,40); 
    text("Para Moverte.",160,90); 
  }
  if(ControlState >= 1){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-z-icon.png",60,125,40,40);
    text("Confirmar/Interactuar.",160,155);
  }
  if(ControlState >= 2){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-x-icon.png",60,170,40,40);
    text("Cancelar/Correr.",160,200);
  }
  if(ControlState >= 3){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-a-icon.png",60,215,40,40);
    text("Abrir/Cerrar menu.",160,245);
  }
  text("Pulsa",160-textWidth("Pulsa")/2,305);
  if(ControlState == 0){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/arrow-down-icon.png",180,280,40,40);
  }else if(ControlState == 1){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-z-icon.png",180,280,40,40);
  }else if(ControlState == 2){
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-x-icon.png",180,280,40,40);
  }else{
    image("http://icons.iconarchive.com/icons/chromatix/keyboard-keys/128/letter-a-icon.png",180,280,40,40);
  }
  ArrowHead(255,17);
}
void render(){ 
  stroke(0);
  background(0); 
  //MainMenuState = 2;
  if(MainMenuState >= 1){    
    if(!Interacting && !Animation.AnimatingE && !Fighting){             
      if(aPressed && Submenu==0 && SubMenu == 0){     
        c++;
        MenuScreen=1;
        ArrowX=3;
        ArrowY=16;
        aPressed=false;
      }
      if(c==1){
        Menu();      
      }else if(c>1){
        c=0;
        Stop=false;
      }     	
    }
    if(!Stop){
      RenderStages(); 
    }
  }
  if(MainMenuState == -1){
    Controls();
    Interacting = true;
  }
  if(MainMenuState == 0){ 
    image("https://1.bp.blogspot.com/--C6t_sEUYb0/XDNURaGPR2I/AAAAAAAAAZA/vu-UwF0ya_seWh-SCQzGcbDRoaUrDKyjACLcBGAs/s1600/TitleScreenProvisional.png",0,0,320,320);
    ArrowHead(0,17);   
    fill(0);
    text("Beta 1.1",250,310);
  }
  if(MainMenuState == 1){
    Animation.BlackScreen(100);
    if(Animation.BlackTimer == 99.0){
      Animation.BlackTimer = 0;
      MainMenuState = 2;
    }
  }
}

void dibuja(){  
  update();
  render();
}
 
void main(){ 
  println("¡Bienvenido a Hidden Realms! Este es un juego RPG de combate por turnos. Si notas que el juego te va con lag, accede a la pestaña de opciones en el menú y baja la calidad de luz. Si encuentras algún bug, repórtalo a este correo: ");
  ArrowX = 118;
  ArrowY = 290;
  animate(dibuja);
  for(int o = 0; o < 1; o++){
    for(int i = 0; i < sizeOf(Team); i++){
      Team[i].LvlUp();
      Team[i].PreLvl = Team[i].Lvl;
    }
  }
}



