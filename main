	public void Encounters(){
    EncounterTimer+=7;
    stroke(255-EncounterTimer,255-EncounterTimer,255-EncounterTimer,EncounterTimer/320);
    strokeWeight(1);
    if(EncounterTimer <= 320){ 
      for(int i = 0; i < EncounterTimer; i++){
        if(i <= 160){
            line(160,160,160+i,0);
            line(160,160,0,160-i);
            line(160,160,160-i,320);
            line(160,160,320,i+160);
        }else{
            line(160,160,320,i-160);
            line(160,160,0,320-(i-160));
            line(160,160,320-(i-160),320);
            line(160,160,i-160,0);
        }      
      }
    }else{
      fill(0);
      rect(-10,-10,340,340);
    }
    if(EncounterTimer > 420){
      EncounterTimer=0;
      AnimatingE=false;
      Fighting=true;
    }
	}
  public void EnterRoom(double Length, double Length2, double Length3){
        noStroke();
    		Interacting = true;
        if(EnterRoomTimer < 160-1){
          EnterRoomTimer+=160/Length;
          for(double i = 0; i < EnterRoomTimer; i++){
            fill(0,0,0);
            rect(0,0,319,i);
            rect(0,0,i,319);
            rect(0,319-i,319,i);
            rect(319-i,0,i,319);
          }
        }  
        if(EnterRoomTimer >= 160-1.0 && EnterRoomTimer3 == 0.0){ 
          if(EnterRoomTimer2 < Length2-1){ 
            EnterRoomTimer2++;
          }
          fill(0,0,0);
          rect(-10,-10,340,340);
        }
        if(EnterRoomTimer2 >= Length2-1.0){ 
          fill(0,0,0,1-EnterRoomTimer3/Length3); 
          rect(-10,-10,340,340);
          if(EnterRoomTimer3 < Length3-1){ 
            EnterRoomTimer3++;
          }else{
            EnteringRoom = false;
            Interacting = false;
            EnterRoomTimer = 0;
            EnterRoomTimer2 = 0;
            EnterRoomTimer3 = 0;
          }          
        }
  }
