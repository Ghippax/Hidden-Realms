        }else if(ID == 3){
          image("https://2.bp.blogspot.com/-c6bVmMzpPQM/XD0MF3EEqJI/AAAAAAAAAfU/x0CPdtsaWp0rD0neU2fTC9yMuzCGr155gCLcBGAs/s1600/Npc3Down.png",x,y,width,height);
        }else if(ID == 4){
          image("https://2.bp.blogspot.com/-XDMZhXw2XfA/XJUbGEYz8CI/AAAAAAAACOY/Ac1v5np9CFwzSnliTyMZiBz794PJWYc6wCLcBGAs/s1600/Goblin3.png",x,y,width,height);
        } 
      }
    }
    if(CollisionBoxes){ 
    	rect(x,y,width,height/2);
    }   
  }
  public void move(){
  	if(Up){
      CharacterState=2;      
      MovY-=speed;             
    }
    if(Left){
      CharacterState=1;
      MovX-=speed;  
    }
    if(Down){
      CharacterState=-2;
      MovY+=speed;  
    }
    if(Right){
      CharacterState=-1;
      MovX+=speed;  
    } 
  }
  public void speak(int character,int event, Portrait X[], Character A){  
    if(Interact && !Stop){        
    	if(box>sizeOf(Dialogue,3)){
      	box = 0;  
        CurrentBox = 0;
      }
      for(int k=0;k<sizeOf(Dialogue,3);k++){
        if(k==box){
          Interacting=true;
          Write(character,event,box, X[box]);                     
        }
      }  
      if(zPressed){        
        PlusX=0;
        Sentence=0;
        CharacterState=-A.CharacterState;
        box++;    
        CurrentBox++;
        zPressed=false;
      }
      if(box >= 0 && box < sizeOf(Dialogue,3) && Dialogue[character][event][box].Size == 0 ){
        PlusX=0;
        Sentence=0;
        CharacterState=-A.CharacterState;
        if(box == sizeOf(Dialogue,3)-1){
        	Interacting=false;
          Read = false;
        }
        box++;       
        CurrentBox++;
      }
    }
  }
  public void Update(Character X){ 
    if(sqrt(pow(x-X.x,2)+pow(y-X.y,2)) < height+30){ 
      if(ID == 0){
        if( x  < X.x + X.width - TestX && x + width > X.x - TestX &&
          y + height > X.y - TestY && y < X.y + X.height - TestY ){
          Interact=true;
          Collision = true;
        }else{
          Interact=false;
          Collision=false;
        }
      }else if(ID == 999){    
        if(!Interact){
          box = 0;   
        }
        if( x  < X.x + X.width - TestX && x + width > X.x - TestX &&
          y + height > X.y - TestY && y < X.y + X.height - TestY ){
          Collision = true;
        }else{
          Collision = false;
        }
        Interact = true;             
        Collision = false;
      }else{
        if( x  < X.x + X.width - TestX && x + width > X.x - TestX &&
          y + height/2 > X.y - TestY && y - height/2 < X.y - TestY ){
          Interact=true; 
          Collision = true;
        }else{
          Interact=false;
          Collision=false;
        }
      }
      if(X.y-TestY < y ){
          Back=true;
      }else {
          Back=false;
      }  
    }
  }
}

class Stages{
  int Layer1[][];
  int Layer2[][];
  int Layer3[][];
  int Trigger1[][];
  int Trigger2[][];
  int Trigger3[][];
  double PosX[];
  double PosY[];
  Enemy[] StgEnemies;
  Npc[] iTiles;
  Chest[] Chests;
  LightSystem StageLight;
  boolean Events[];
  boolean Initialize=true;
  Chest ClosestChest; 
  public Stages(int Size1,int Size2,int Size3, int Size4, int Size5, int Size6, int Size7,
               int Size8, int r, int g, int b){    
    Trigger1 = new int[Size1][Size2];
    Trigger2 = new int[Size1][Size2];
    Trigger3 = new int[Size1][Size2];
    StgEnemies = new Enemy[Size3];    
    PosX = new double[Size4];
    PosY = new double[Size4];    
    iTiles = new Npc[Size5];
    for(int i = 0; i < Size5; i++){
      iTiles[i] = new Npc(0,0,32,32,0,999);
    }
    Events = new boolean[Size6];
    Chests = new Chest[Size7];
    for(int i = 0; i < Size6; i++){
      Events[i] = false;
    }
    StageLight = new LightSystem(Size8,r,g,b);
  }
