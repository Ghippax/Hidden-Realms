class Light{
  double x; 
  double y;
  double Intensity;
  int id;
  boolean Off;
  Fire LightFire;
  public Light(double x, double y, double Intensity, int id){
    this.x = x;
    this.y = y;
    this.Intensity = Intensity;
    this.id = id;
    LightFire = new Fire(x-5,y-10,0,false);
    if(id == 2){
      Off = true;
    }
  }
  public boolean Collide(Character X, int radius){
    if(Dist(x+CameraX,y+CameraY,X.x,X.y) < radius){
      if(zPressed){
        zPressed = false; 
				return true
      }else{
        return false
      }
    }else{
      return false
    }
  }
  public void render(){
    if(id == 1){ 
      image("https://3.bp.blogspot.com/-TJVRkc-my3Y/XFS8VXyIdjI/AAAAAAAAAjY/pBQ3lpZWGV4M5xf54S9LKFO2NRwmV0ytwCLcBGAs/s1600/65.png",x+CameraX,y+CameraY,6,25);
    }
    if(id == 2){
      image("https://2.bp.blogspot.com/--bJgkzwz3Tk/XFsTxSlgjwI/AAAAAAAAAk4/gn50oT_upI0td74bINaRX_rxQeDfOTgZwCLcBGAs/s1600/66.png",x+CameraX,y+CameraY,6,30);
      if(!Off){
        LightFire.render();
      }
    }
  }
  public void OnOff(){
    if(Off){
    	Off = false;
    }else{
    	Off = true;
    }    
  }
  public double LightValue(double X, double Y){
    if(ShowLightSources){ 
      fill(255,0,0);
      rect(x+CameraX,y+CameraY,10,10);
      noFill();
    }
    if(Off){
      return 2000
    }else{ 
      if(id == 0){
        if(CurrentStage != 1){
           return Dist(160,160,X,Y)/CharacterLight
        }else{
          return Dist(160,160,X,Y)/Intensity
        }        
      }else{
        return Dist(x+CameraX,y+CameraY,X,Y)/Intensity
      }    
    }
  }
}
class LightSystem{
  Light[] LightSources;
  int r;
  int g;
  int b;
  public LightSystem(int Size, int r, int g, int b){ 
    this.r = r;
    this.g = g;
    this.b = b;
    LightSources = new Light[Size];
  }
  public double Alpha(double X, double Y){
    double OverallA;
    for(int i = 0; i < sizeOf(LightSources); i++){
      OverallA += 1/LightSources[i].LightValue(X,Y);
    }    
    if(1/OverallA == 0.0){
      return 0.00001
    }else if(1/OverallA > 1.0){ 
      return 1
    }else{ 
    	return 1/OverallA
    }
  }
}

