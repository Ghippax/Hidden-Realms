  public boolean Detect(int Trigger[][], int Effect, Character X){
  	for(int i=0;i<sizeOf(Trigger);i++){ 
      TileX=CameraX;
      for(int o=0;o<sizeOf(Trigger,2);o++){ 
        TileX+=TileSize;  
        if( X.x + X.width - TestX > TileX - TileSize  && X.x - TestX < TileX &&
              X.y + X.height - TestY > TileY && X.y - TestY < TileY + TileSize){
          CurrentTileX = TileX-TileSize;
          CurrentTileY = TileY;
          CurrentPosI = i;
          CurrentPosO = o;
          if(Trigger[i][o]==Effect){           
       			return true; 
          }
        }        
      }     
      TileY+=TileSize;
    }  
    
    TileY=CameraY; 
    return false;  
  }
  public boolean DetectThis(int Trigger[][], int Effect, Character X, int First, int Second){
  	for(int i=0;i<sizeOf(Trigger);i++){ 
      TileX=CameraX;
      for(int o=0;o<sizeOf(Trigger,2);o++){ 
        TileX+=TileSize;  
        if( X.x + X.width - TestX  > TileX - TileSize   && X.x - TestX < TileX &&
              X.y + X.height - TestY > TileY && X.y - TestY < TileY + TileSize && First == i && Second == o){
          CurrentTileX = TileX-TileSize;
          CurrentTileY = TileY;
          fill(255,0,0,0.5);
          if(SpecialTiles){
            rect(CurrentTileX,CurrentTileY,TileSize,TileSize);
          }
          if(Trigger[i][o]==Effect){           
       			return true; 
          }
        }        
      }     
      TileY+=TileSize;
    }  
    
    TileY=CameraY; 
    return false;  
  }
  public boolean Collide(int Trigger[][], Character X){
    if(Detect(Trigger,1,X)){
 			return CanMove=false;
    }else{
      return CanMove=true;
    }
  }
  public void addTrigger(int i, int o, int Effect, int[][] Trigger){
    Trigger[i][o] = Effect;
  }
  public void addExit(int First, int Second, int Exit, int[][] Trigger, Character X, double TransportX, double TransportY, Stages Destination){
    addTrigger(First,Second,9,Trigger);
    if(abs(CurrentPosI-First) < 3 && abs(CurrentPosO-Second) < 3){ 
      pasos = round(random(64,255));
      if(DetectThis(Trigger,9,X,First,Second)){
        Animation.EnteringRoom = true;
        if(Animation.EnterRoomTimer2 > 1){ 
          PrevStage=CurrentStage;
          CurrentStage=Exit;
          Destination.Initialize=true;
          CameraX = TransportX;
          CameraY = TransportY;
        }           
      }
    }
  }
  public void addText(int First, int Second, int Event, int[][] Trigger, Character X, int iTile, Portrait[] Portraits){       
    addTrigger(First,Second,3,Trigger);
    iTiles[iTile].Update(X);
    fill(255);
		if(DetectThis(Trigger,3,X,First,Second)){         
      iTiles[iTile].x = CurrentTileX;
      iTiles[iTile].y = CurrentTileY;
      
      if(iTiles[iTile].Read){     
      	iTiles[iTile].speak(1,Event,Portraits,X);
      }
    }
  }
  public void addEvent(int First, int Second, int Event, int[][] Trigger, Character X){       
    addTrigger(First,Second,4,Trigger);
    if(DetectThis(Trigger,4,X,First,Second)){
      Events[Event] = true;
    }
  }
  public void Signs(int[][] Trigger, Character X, int Event){   
    if(Detect(Trigger,2,X)){     
      Sign.CharacterState = -X.CharacterState;        
      Sign.x = CurrentTileX;
      Sign.y = CurrentTileY;
      Portrait[] PortSign = new Portrait[sizeOf(Dialogue,3)];
      for(int i = 0; i < sizeOf(PortSign); i++){
        PortSign[i] = Using.Port;
      }
      for(int i = 0; i < sizeOf(PosX); i++){
        if(X.x + X.width - TestX > PosX[i] - TileSize && X.x - TestX < PosX[i] + TileSize &&
              X.y + X.height - TestY > PosY[i] && X.y - TestY < PosY[i] + TileSize){
          Sign.speak(Event,i, PortSign, X);
          if(SpecialTiles){
            fill(0,0,0,0.5);
            rect(PosX[i],PosY[i],TileSize,TileSize);
          }
        }
      }      
    }
  }
  public void LightUp(){
    strokeWeight(2);
    for(int i = 0; i <= 320; i+=320/LightQuality){
      for(int o = 0; o <= 320; o+=320/LightQuality){
        fill(StageLight.r,StageLight.g,StageLight.b,StageLight.Alpha(i,o));     
        noStroke();        
        rect(i,o,320/LightQuality,320/LightQuality);
      }
    }
  }
}

