  public void DefAnimation(int ID, Member A, Enemy B){
    DefTimer++;
    if(DefTimer <= 5 ){ 
    	xHeight+= A.height/5;
    }
    if(DefTimer == 1){ 
    	xHeight = -A.height;
    }
   	if(DefTimer >= 20){    
    	DefTimer = 0;
      xHeight = 0;
    }else{
        fill(169,169,169);
        noStroke();       
        triangle(A.x - 15 -20, A.y -15 -xHeight, A.x - 15 -20 +0.5, A.y + A.height, A.x -20 +0.5, A.y -xHeight);
        triangle(A.x -20, A.y + A.height + 15, A.x - 15 -20, A.y + A.height, A.x -20, A.y -xHeight);
        fill(98,98,98);
        triangle(A.x - 15 -20, A.y -15 -xHeight, A.x -20, A.y -15 -xHeight, A.x -20, A.y -xHeight);
        triangle(A.x + 15 -20, A.y -xHeight, A.x -20, A.y -15 -xHeight, A.x -20, A.y -xHeight);
        rect(A.x -20, A.y -xHeight, 15, A.height +15 +xHeight);
      }
  }
  public void WhiteScreen(int Lenght){
    if(WhiteTimer < Lenght){
    	WhiteTimer++;  
    }else{
      WhiteTimer = 0;
      White = false;      
    }    
    fill(255,255,255,WhiteTimer/Lenght);
    rect(-10,-10,330,330);
  }
  public void Flash(int Lenght){
    if(FlashTimer < Lenght){
    	FlashTimer++;  
    }   
    fill(255,255,255,1-FlashTimer/(Lenght+1));
    rect(-10,-10,330,330);
  }
  public void BlackScreen(int Lenght){
    if(BlackTimer < Lenght-1){ 
  		BlackTimer++;
    }
    fill(0,0,0,1-BlackTimer/Lenght);
    rect(-10,-10,340,340);
  }
  public void NewWorld(double speed){
    if(NewWorldTimer1 <= 160){
    	NewWorldTimer1+=speed+NewWorldTimer1/10; 
    }
    if(NewWorldTimer1 >= 160 && NewWorldTimer2 <= 160){
      NewWorldTimer2 +=speed+NewWorldTimer2/10;
    }
    if(NewWorldTimer2 >= 160 && NewWorldTimer3 <= 15){
      NewWorldTimer3++;
    }
    if(NewWorldTimer3 >= 15 && NewWorldTimer4 <=100){
      NewWorldTimer4+=2;
    }
    for(double i = 0; i < NewWorldTimer1; i++){
      stroke(0,0,0,(NewWorldTimer1/160)/NewWorldTimer4);
      line(i,160-sqrt(i*(160-i)),i,320-sqrt(i*(320-i)));
      line(160+sqrt(i*(160-i)),i,sqrt(i*(320-i)),i);
      line(320-i,160+sqrt(i*(160-i)),320-i,sqrt(i*(320-i)));
      line(320-sqrt(i*(320-i)),320-i,160-sqrt(i*(160-i)),320-i);
    }
    if(NewWorldTimer1 >= 160){ 
      for(double i = 0; i < NewWorldTimer2; i++){
        stroke(0,0,0,(NewWorldTimer2/160)/NewWorldTimer4);
        line((160-i),160-sqrt((160-i)*(160-(160-i))),(160-i),160-sqrt(pow(160,2)-pow((160-i),2)));
        line(160+sqrt(pow(160,2)-pow((160-i),2)),(160-i),160+sqrt((160-i)*(160-(160-i))),(160-i));    
        line(320-(160-i),160+sqrt(pow(160,2)-pow((160-i),2)),320-(160-i),160+sqrt((160-i)*(160-(160-i))));   
        line(160-sqrt((160-i)*(160-(160-i))),320-(160-i),160-sqrt(pow(160,2)-pow((160-i),2)),320-(160-i));    
      }
    }
    point(160,160);
    if(NewWorldTimer4 != 101.0){
      Interacting = true;
    }else{
      Interacting = false;
    }
  }
