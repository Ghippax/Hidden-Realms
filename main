int BossStage = 16;
boolean Escaped = false;
void NextCh(){
	while(CurrentCharacter+q < sizeOf(Team) && Team[CurrentCharacter+q].HP <= 0 ){                    
    Queque[CurrentCharacter+q] = Ignore;
    Team[CurrentCharacter+q].TargetM = Team[CurrentCharacter+q];
    Team[CurrentCharacter+q].TargetE = null;
    q++;
  }    
  if(CurrentCharacter + 1 + q > sizeOf(Team)){
    BattlePhase = 2;
  }else{ 
  	CurrentCharacter += 1+q;
  }
  q=0;  
}
int AllEnemiesNLootTillThis;
void Battle(){  
  if(!Lost && !Win){ 
    noFill();
    image("https://3.bp.blogspot.com/-x5UpsH5pi1M/XFsl6XXbgbI/AAAAAAAAAlc/oS1xdNb01o4DH8mwRwm3EasNjfi5Q-j3QCLcBGAs/s320/battleback5.png",0,0,320,320);
    for(int i = 0; i < sizeOf(Team); i++){   
      if(Team[CurrentCharacter-1] == Team[i] && BattlePhase == 1){
      	Team[i].Render(240+i*10-20,80+i*50);  
      }else{
      	Team[i].Render(240+i*10,80+i*50);  
      }    	      
    }
    for(int i = 0; i < sizeOf(EnemyList); i++){
      if(EnemyList[i].HP > 0){ 
        if(i % 2 == 0){ 
          EnemyList[i].Render(20,30+i*50);          
        }else{
          EnemyList[i].Render(50,30+i*50);
        }
      }
    }  
    switch(BattlePhase){ 
      case 1:
        Brect(164,220,155,99);
        textSize(10);
        fill(TextColor);
        switch(BattleMenu){
          case 1:  	
            text("Atacar",183,240);
            text("Defender",183,261);
            text("Objetos",183,283);
          	if(CurrentStage != BossStage){ 
            	text("Huir",183,305);
            }
            if(Team[CurrentCharacter-1].ID==1){      
              text("Magia",253,240);
              text("Negra",253,251);
            }
            if(Team[CurrentCharacter-1].ID==2){  
              text("Engaño",253,240);
            }
            if(Team[CurrentCharacter-1].ID==3){  
              text("Hab.",253,240);
              text("Espada",253,251);
            }
            if(Team[CurrentCharacter-1].ID==4){  
              text("Magia",253,240);
              text("Blanca",253,251);
            }
            if(zPressed && ArrowY==227.0 && ArrowX==165.0){BattleMenu=2; zPressed=false;}
            if(zPressed && ArrowY==249.0 && ArrowX==165.0){BattleMenu=3; zPressed=false;}
            if(zPressed && ArrowY==271.0 && ArrowX==165.0){BattleMenu=4; zPressed=false; ArrowY=227.5; ArrowX=165.0; ObjectCamera=0;}
            if(zPressed && ArrowY==293.0 && ArrowX==165.0 && !Escaped && CurrentStage != BossStage){BattleMenu=5; zPressed=false; Escaped = true;}
            if(zPressed && ArrowY==227.0 && ArrowX==235.0){BattleMenu=6; zPressed=false; ArrowY=227.5; ArrowX=165.0; MovementCamera=0;}
            if(ArrowLeft && ArrowX > 165){
              ArrowX-=70;
              ArrowLeft=false;
            }
            if(ArrowRight&& ArrowX < 235 && ArrowY <= 227){
              ArrowX+=70;
              ArrowRight=false;
            }
            if(ArrowUp && ArrowY > 227){
              ArrowY-=22;
              ArrowUp=false;
            }
            if(ArrowDown && ArrowY < 293 && ArrowX < 235){
              ArrowY+=22;
              ArrowDown=false;
            }
          	if(xPressed && CurrentCharacter > 1){                           
              while(CurrentCharacter-2-q > 0 && Team[CurrentCharacter-2-q].HP <= 0){          
                q++;
              }                   	
              if(Queque[CurrentCharacter-2-q] == UseItem){
                UseItem.BattleItem.Quantity += 1;	  
              }
              xPressed=false;    
              if(CurrentCharacter-2-q == 0 && Team[0].HP <= 0){              	        
              }else{
                CurrentCharacter -= 1+q;
              }
              q=0;
            }                       	
          break
          case 2:
            Queque[CurrentCharacter-1] = Attack;
            BattleMenu = 7;
            ArrowY = 227.5;
            Selected = Attack;
          break
          case 3:
            Queque[CurrentCharacter-1] = Defend;
            Team[CurrentCharacter-1].TargetM = Team[CurrentCharacter-1];
            Team[CurrentCharacter-1].TargetE = null;
          	NextCh();
            BattleMenu=1;
          break
          case 4:
            Selected = UseItem;
            for(int i = 0; i < sizeOf(ObjectList);i++){ 
              if(240+i*22 + ObjectCamera > 222.5 && ObjectList[i].Quantity > 0){ 
                text(ObjectList[i].Name,183,241+i*22 + ObjectCamera);
                text(ObjectList[i].Quantity,273,241+i*22 + ObjectCamera);
              }  
              if(245 + i * 22 - 17.5 + ObjectCamera==ArrowY && ObjectList[i].Quantity > 0 && zPressed==true){
                zPressed = false;          
                UseItem.BattleItem = ObjectList[i];
                UseItem.BattleItem.Quantity -= 1;
                ArrowY = 227.5;
                BattleMenu = 7;  
              }
            }
