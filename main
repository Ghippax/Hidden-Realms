        }else{
          image("https://3.bp.blogspot.com/-27F4_yzgWKM/XFSFr-RhEuI/AAAAAAAAAi8/TwqR3zqq9_4ToA7bgYnCkW-DyoAD9gVcwCLcBGAs/s1600/chest0003.png",x,y,width,height);
        }        
      }
    }
    if(Appearance == 4){
      if(!Looted){ 
        ChestStar.x = PosX + TileSize/2; ChestStar.y = PosY + TileSize/2;
        ChestStar.Animate();
      }
    }
  }
	public boolean Collide(Character X){        
    if( x < X.x + X.width - TestX && x + width > X.x - TestX &&
          y + height/2 > X.y - TestY && y - height*1.5 < X.y - TestY ){
      return true;          
    }else{ 
      return false;
    }
  }
  public void Solid(Character X, boolean Closest){
    if(Closest){
    	if(!(Appearance == 4)){ 
        if(Collide(X)){
          Collision = true;
        }else{
          Collision = false;
        }
    	}else{
        if(!Looted){ 
          if(Collide(X)){
            Collision = true;
          }else{
            Collision = false;
          }
        }else{
          Collision = false;
        }
    	}
    }
  }
  int ChestBox = -1;
  public void Loot(Character X){    
    if(Collide(X) && !Looted){
      if(ChestBox == 0){ 
      	Brect(1,230,318,89);
        fill(TextColor);
        textSize(11);
        if(ID == -1){
          text("¡Has obtenido una antorcha mágica!",320/2-textWidth("¡Has obtenido una antorcha mágica!")/2,280);
        }else if(ID == -2){ 
          text("Has obtenido "+RewardQuantity+" Créditos",320/2-textWidth("Has obtenido "+RewardQuantity+" Créditos")/2,280);
        }else{
          text("Has obtenido "+RewardQuantity+" "+Reward.Name,320/2-textWidth("Has obtenido "+RewardQuantity+" "+Reward.Name)/2,280);
        }        
      }
      if(ChestBox == 1){
        ChestBox = -1;
        if(ID == -1){
          CharacterLight = 150;
        }else if(ID == -2){ 
          Money += RewardQuantity;
        }else{ 
        	Reward.Quantity += RewardQuantity;
        }
        Looted = true;
        Interacting = false;
      }      
      if(zPressed){
        if(ChestBox > 1){
          ChestBox = -1;
        }
        Interacting = true;
        ChestBox++;    
        zPressed=false;
      }
    }
  }
}

class Movement{
  String Name;
  int Cost;
  int Power;
  int ID;  
  String Type;
  Item BattleItem;
  public Movement( int SetID, String SetName, int SetPower, int SetCost, String SetType){
    Name=SetName;
    Power = SetPower;
    Cost=SetCost;
    ID = SetID;
    Type = SetType;
  }
  public void UseE(Enemy User){
  	if(User.TargetM.Defending){ 
      if(Type == "Fisico"){ 
  			User.Damage = (Power * User.Atk / User.TargetM.Def)/2;
      }else if(Type == "Magico"){
        User.Damage = (Power * User.Mag / User.TargetM.Def)/2;
      }     
    }else{
      if(Type == "Fisico"){ 
  			User.Damage = Power * User.Atk / User.TargetM.Def;
      }else if(Type == "Magico"){
        User.Damage = Power * User.Mag / User.TargetM.Def;
      }     
    }
    User.TargetM.HP -= User.Damage;
  }
  public void UseM(Member User){
   	if(ID != 0 && ID != 1001 && ID != 1000){ 
      User.MP -=Cost;       
      if(User.TargetM == null){
        if(Type == "Fisico"){  
  				User.Damage = Power * User.Atk / User.TargetE.Def;
        }else if(Type == "Magico"){
					User.Damage = Power * User.Mag / User.TargetE.Def;
        }      	
        User.TargetE.HP -= User.Damage;           
      }else if(User.TargetE == null){
        if(Type == "Fisico"){ 
  				User.Damage = Power * User.Atk / User.TargetM.Def;
        }else if(Type == "Magico"){
					User.Damage = Power * User.Mag / User.TargetM.Def;
        }        
        User.TargetM.HP -= User.Damage; 
      } 
    }
    if(ID == 1001){
      if(User.TargetE == null){ 
        if(BattleItem.HPRestore > 0){
          if(User.TargetM.HP + BattleItem.HPRestore <= User.TargetM.MaxHP){
  					User.Damage = BattleItem.HPRestore;
          }else{
            User.Damage = User.TargetM.MaxHP - User.TargetM.HP;
          }
        }else if(BattleItem.MPRestore > 0){
          if(User.TargetM.MP + BattleItem.MPRestore <= User.TargetM.MaxMP){
  					User.Damage = BattleItem.MPRestore;
          }else{
            User.Damage = User.TargetM.MaxMP - User.TargetM.MP;
          }
        }
    		BattleItem.UseM(User.TargetM);        
      }else if(User.TargetM == null){   
        if(BattleItem.HPRestore > 0){
          if(User.TargetE.HP + BattleItem.HPRestore <= User.TargetE.MaxHP){
  					User.Damage = BattleItem.HPRestore;
          }else{
            User.Damage = User.TargetE.MaxHP - User.TargetE.HP;
          }
        }else if(BattleItem.MPRestore > 0){
          if(User.TargetE.MP + BattleItem.MPRestore <= User.TargetE.MaxMP){
  					User.Damage = BattleItem.MPRestore;
          }else{
            User.Damage = User.TargetE.MaxMP - User.TargetE.MP;
          }
        }
        
        BattleItem.AffectedE = User.TargetE;
      	BattleItem.UseE();        
      }
    }
  }
}

