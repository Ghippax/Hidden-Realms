void ArrowHead(int color, int size){
  fill(color);
  noStroke();
  if(Floating >= 4){
  	Float = false; 
  }
  if(Floating <= -1){
    Float = true;
  }
  if(Float){
  	Floating+=0.4;  
  }else{
  	Floating-=0.4;  
  }        
  triangle(ArrowX+size/2-Floating,ArrowY+size/5,ArrowX+size-Floating,ArrowY+size/2,ArrowX+size/2-Floating,ArrowY+size-size/5);
}

void Candle(){
	if(Oscilate > 3){
    Oscilating = false;     
  }
  if(Oscilate < -3){
    Oscilating = true;      
  }
  if(Oscilating){
    Oscilate+=0.15;
  }else{ 
    Oscilate-=0.15;
  }
  CharacterLight += round(Oscilate);
}
