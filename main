class Npc{
	double x;
  double y;
  double MovX;
  double MovY;
  int width;
  int height;
  int speed;
  int CharacterState;
  int ID;
  int CurrentBox;
  int box=-1;
  boolean Interact;
  boolean Right;
  boolean Left;
  boolean Up;
  boolean Down;
  boolean Read = true;
  public Npc(double CharacterX,double CharacterY,int CharacterWidth,int CharacterHeight,int CharacterSpeed, int SetID){
    x=CharacterX;
    y=CharacterY;
    MovX=x;
    MovY=y;
    width=CharacterWidth;
    height=CharacterHeight;
    speed=CharacterSpeed;
    ID = SetID;
  }
  public void render(){
    y=CameraY+MovY;
    x=CameraX+MovX;
    if(ID >= 1){ 
      if(CharacterState==1){
        if(ID == 1){
          image("https://1.bp.blogspot.com/-NYdZECHjRVk/XD0MFEzVAeI/AAAAAAAAAfE/PXPSdfzLikwlBl4TJSqLrTON3x6ATs12wCLcBGAs/s1600/Npc2Left.png",x,y,width,height);
        }else if(ID == 2){
          image("https://4.bp.blogspot.com/-0l5U2CsHj2Q/XD0MDsKWpDI/AAAAAAAAAe8/EJ-BjfaVR-M0MIiqeQ9fkwDTOyS8JC7owCLcBGAs/s1600/Npc1Left.png",x,y,width,height);
        }else if(ID == 3){
          image("https://4.bp.blogspot.com/-3v54hqMG2EM/XD0MGfoZqoI/AAAAAAAAAfY/Vl5ZDwHRHI0hXElzn_-c7p8fcjX5byHdgCLcBGAs/s1600/Npc3Left.png",x,y,width,height);
        }else if(ID == 4){
          image("https://4.bp.blogspot.com/-GFN6iVsiL-U/XJUbGEidhNI/AAAAAAAACOQ/UvtcyRz0jH0wGCnBZIdQHLYFRFHkuUDDACLcBGAs/s1600/Goblin2.png",x,y,width,height);
        }          
      }
      if(CharacterState==-1){
        if(ID == 1){
          image("https://4.bp.blogspot.com/-c91Rsp_VZf4/XD0MFOzT7MI/AAAAAAAAAfI/vKU03yCrFmwVGIia1FsnP2WVBHJg0WrngCLcBGAs/s1600/Npc2Right.png",x,y,width,height);
        }else if(ID == 2){
          image("https://2.bp.blogspot.com/-b8ZexOEPIFI/XD0MDWdyiGI/AAAAAAAAAe0/mt-cCIjvUg0UJoU_ZYUT0WN4Gl1BVZX2gCLcBGAs/s1600/Npc1Right.png",x,y,width,height);
        }else if(ID == 3){
          image("https://2.bp.blogspot.com/-GlnSZS5ubok/XD0MGsPtJNI/AAAAAAAAAfc/fHVfGZhA2Zcrs-UcvcNp6vjxcYvEtYNPQCLcBGAs/s1600/Npc3Right.png",x,y,width,height);
        }else if(ID == 4){
          image("https://1.bp.blogspot.com/-26Y3KQxbKVA/XJUbG6q4jnI/AAAAAAAACOc/iJJlsUVVZpI3RWEZW3tZoBNB1RKpCo44gCLcBGAs/s1600/Goblin4.png",x,y,width,height);
        } 
      }
      if(CharacterState==2){
        if(ID == 1){
          image("https://4.bp.blogspot.com/-A6sq6j6dN-M/XD0MF9_WZYI/AAAAAAAAAfQ/7d7M5rHGdTYUvnJ5H_LQvwjdeV8Uqx5iwCLcBGAs/s1600/Npc2Up.png",x,y,width,height);
