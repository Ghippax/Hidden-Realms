  int PlusDef;
  int PlusMag;
	public Item(int SetID, String SetName, int SetRarity, int SetLines, int SetPlusAtk, int SetPlusDef, int SetPlusMag,
             int SetHPRestore, int SetMPRestore, String SetPlace){
  	ID = SetID;   
    Name = SetName; 
    Rarity = SetRarity;
    ToolTip = new String[SetLines];
    PlusAtk = SetPlusAtk;
    PlusDef = SetPlusDef;
    PlusMag = SetPlusMag;
    HPRestore = SetHPRestore;
    MPRestore = SetMPRestore;
    Place = SetPlace;
  }  
  public void UseM(Member Affected){
    Affected.HP += HPRestore;
    Affected.MP += MPRestore;
    if(Affected.HP > Affected.MaxHP){Affected.HP=Affected.MaxHP;}
    if(Affected.MP > Affected.MaxMP){Affected.MP=Affected.MaxMP;}
  }	
  public void UseE(){
    AffectedE.HP += HPRestore;
    AffectedE.MP += MPRestore;
    if(AffectedE.HP > AffectedE.MaxHP){AffectedE.HP=AffectedE.MaxHP;}
    if(AffectedE.MP > AffectedE.MaxMP){AffectedE.MP=AffectedE.MaxMP;}
  }	
  public void Equip(Member Affected, int Type){
    if(Type == 1){
     	Affected.Armor.Quantity += 1;
     	Affected.Mag = Affected.RawMag; 
     	Affected.Atk = Affected.RawAtk;
      Affected.Def = Affected.RawDef;
      Affected.Armor = this;                
    }else if(Type == 2){
      Affected.Weapon.Quantity += 1;
      Affected.Mag = Affected.RawMag; 
      Affected.Atk = Affected.RawAtk;
      Affected.Def = Affected.RawDef;
      Affected.Weapon = this;            
    }else if(Type == 3){
      Affected.Accessory.Quantity += 1;
      Affected.Mag = Affected.RawMag; 
      Affected.Atk = Affected.RawAtk;
      Affected.Def = Affected.RawDef;
      Affected.Accessory = this;         
    }
    Affected.Atk += Affected.Weapon.PlusAtk;
    Affected.Mag += Affected.Weapon.PlusMag;
    Affected.Def += Affected.Weapon.PlusDef;
    Affected.Atk += Affected.Armor.PlusAtk;
    Affected.Mag += Affected.Armor.PlusMag;
    Affected.Def += Affected.Armor.PlusDef;
    Affected.Atk += Affected.Accessory.PlusAtk;
    Affected.Mag += Affected.Accessory.PlusMag;
    Affected.Def += Affected.Accessory.PlusDef;
  }
}

class Chest{
  int ID;
  double x;
  double y;
