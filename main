class PulsatingBall{
  double x;
  double y;
  double Size;
  int r;
  int g;
  int b;
  int Intesity;
  double Age;
  boolean Pulsating;
  public PulsatingBall(double SetX, double SetY, double SetSize, int SetR, int SetG, int SetB, int SetIntesity){
    x = SetX;
    y = SetY;
    Size = SetSize;
    r = SetR;
    g = SetG;
    b = SetB;
    Intesity = SetIntesity;
  }
  public void render(double Speed, double MaxLim, double MinLim){
    noStroke(); 
    if(Age >= MaxLim){
      Pulsating = false;
    }
    if(Age <= MinLim){
      Pulsating = true;
    }
    if(Pulsating){
      Age+=Speed;
    }else{
      Age-=Speed;
    }  
    fill(r,g,b,Age);
    ellipse(x+CameraX,y+CameraY,Size+Size/10,Size+Size/10);
    ellipse(x+CameraX,y+CameraY,Size+Size/5,Size+Size/5);
    for(double i = 0; i < Size; i++){
      fill(round(r+Intesity/(Size)*i),round(g+Intesity/(Size)*i),round(b+Intesity/(Size)*i));
      ellipse(x+CameraX,y+CameraY,Size-i,Size-i);
    }
  }
}

class Star{
  double x;
  double y;
  double Size;
  int r;
  int g;
  int b;
  boolean Rotated;
  double StarTimer;
  double Speed;
  public Star(double SetX, double SetY, double SetSize, int SetR, int SetG, int SetB, double SetSpeed){
    x = SetX;
    y = SetY;
    Size = SetSize;
    r = SetR;
		g = SetG;
    b = SetB;
    Speed = SetSpeed;
  }
  public void Render(double StarX, double StarY, double StarSize, boolean Rotated){
    StarX=StarX-StarSize/2;
    StarY=StarY-StarSize/2; 
    stroke(r,g,b);
    fill(r,g,b);
    if(!Rotated){    
      //Cuerpo
      rect(StarX,StarY,StarSize,StarSize);    
      //Picos
      triangle(StarX,StarY,StarX+StarSize,StarY,StarX+StarSize/2,StarY-StarSize);
      triangle(StarX+StarSize,StarY,StarX+StarSize*2,StarY+StarSize/2,StarX+StarSize,StarY+StarSize);
      triangle(StarX,StarY,StarX-StarSize,StarY+StarSize/2,StarX,StarY+StarSize);
      triangle(StarX+StarSize,StarY+StarSize,StarX,StarY+StarSize,StarX+StarSize/2,StarY+StarSize*2);
    }else{
      //Cuerpo
      triangle(StarX+StarSize/2,StarY,StarX+StarSize/2,StarY+StarSize,StarX,StarY+StarSize/2);
      triangle(StarX+StarSize/2,StarY,StarX+StarSize/2,StarY+StarSize,StarX+StarSize,StarY+StarSize/2);
      //Picos
      triangle(StarX+StarSize/2,StarY,StarX,StarY+StarSize/2,StarX-StarSize/2,StarY-StarSize/2);
      triangle(StarX+StarSize/2,StarY,StarX+StarSize,StarY+StarSize/2,StarX+StarSize*1.5,StarY-StarSize/2);
      triangle(StarX,StarY+StarSize/2,StarX+StarSize/2,StarY+StarSize,StarX-StarSize/2,StarY+StarSize*1.5);
      triangle(StarX+StarSize/2,StarY+StarSize,StarX+StarSize,StarY+StarSize/2,StarX+StarSize*1.5,StarY+StarSize*1.5);
    }
  }
  public void Animate(){
    StarTimer+=Speed;   
    if(StarTimer > 2){
    	StarTimer = 0;
    }
    if(StarTimer <= 1){
      Render(CameraX+x,CameraY+y,Size/(StarTimer*4+1), true);
    }else if(StarTimer <= 2){
      Render(CameraX+x,CameraY+y,Size/((StarTimer-1)*4+1), false);
    }
  }
}

