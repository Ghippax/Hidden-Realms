    Mag += 1; RawMag += 1;
    Exp = 0;
    ExpToNextLvl = 250 * Lvl;
    for(int i = 0; i < sizeOf(MovementSet); i++){
      if(Lvl >= UnlockLvl[i]){
        Unlocked[i] = true;
      }
    }
  }
  public void Learn(double x, double y){    	
    for(int i = 0; i < sizeOf(MovementSet); i++){
      if(Lvl == UnlockLvl[i]){
        Brect(x,y,100,40);  
        fill(TextColor);
        textSize(10);
        text("Aprende:",x+10,y+15);
        text(MovementSet[i].Name,x+10,y+30);  
      }
    }
  }
}

