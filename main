Animations Animation;
Animation = new Animations(0);

class Character{
  double x;
  double y;
  int width;
  int height;
  int speed;
  int CharacterState; 
  int ID;
  Portrait Port; 
  public Character(double CharacterX,double CharacterY,int CharacterWidth,int CharacterHeight,int CharacterSpeed, int SetID){
    x=CharacterX;
    y=CharacterY;
    width=CharacterWidth;
    height=CharacterHeight;
    speed=CharacterSpeed;
    ID = SetID;
    if(ID == 1){
      Port = XavierChild;
    }else if(ID == 2){
      Port = XavierAdult;
    }
  }
  public void render(){   
    if(xPressed){ 
    	Animation.Walk(ID,1.4,this);    
    }else{
      Animation.Walk(ID,2,this);
    }
    if(CollisionBoxes){ 
    	rect(x,y,width,height/2);
    }    
  }
  public void GetTest(int Predict){
  	switch(Predict){
      case 1:
        TestX=0;
        TestY=speed;
      break
      case 2:
        TestY=0;
        TestX=speed;
      break
      case 3:
        TestX=0;
        TestY=-speed;
      break
      case 4:
        TestY=0;
        TestX=-speed;
      break
    } 
  }  
  public void move(){     
  	if(upPressed==true && !leftPressed && !rightPressed && !downPressed){       
      CharacterState=2;
      if(CanMove && !Collision){
        CameraY+=speed;
      }else{
        Animation.WalkTimer=0;
      }
    }
    if(leftPressed==true && !rightPressed && !upPressed && !downPressed){
      CharacterState=1;
      if(CanMove && !Collision){ 
        CameraX+=speed;        
      }else{
        Animation.WalkTimer=0;
      }
    }
    if(downPressed==true && !upPressed && !rightPressed && !leftPressed){
      CharacterState=-2;
      if(CanMove && !Collision){
        CameraY-=speed;
      }else{
        Animation.WalkTimer=0;
      }
    }
    if(rightPressed==true && !upPressed && !downPressed && !leftPressed){      
      CharacterState=-1;
      if(CanMove && !Collision){  
        CameraX-=speed;
      }else{
        Animation.WalkTimer=0;
      }
    }      	   
  }
  
}
void Write(int character,int Event,int Box, Portrait X){
	strokeWeight(2);
  textSize(9); 
  Brect(1,230,79,89);
  Brect2(80,230,238,89);
  X.Render(10.5,236,60);
  fill(TextColor);
  text(X.Name,6,313);
  PlusX+=TextSpeed;
  noStroke();
  textSize(11);
  if(Sentence>=3){
    fill(TextColor);
  	text(Dialogue[character][Event][Box].Array[3],90,310);  
    Brect2(80+PlusX,230,textWidth(Dialogue[character][Event][Box].Array[Sentence])+20,89);
  }
  if(Sentence>=2){
    fill(TextColor);
  	text(Dialogue[character][Event][Box].Array[2],90,290);  
    if(Sentence==2){Brect2(80+PlusX,230,textWidth(Dialogue[character][Event][Box].Array[Sentence])+20,89);}
  }
  if(Sentence>=1){
  	fill(TextColor);    	      
  	text(Dialogue[character][Event][Box].Array[1],90,270);  
    if(Sentence==1){Brect2(80+PlusX,230,textWidth(Dialogue[character][Event][Box].Array[Sentence])+20,89);}  
  }
  if(Sentence>=0){
    fill(TextColor);    	      
  	text(Dialogue[character][Event][Box].Array[0],90,250);
    if(Sentence==0){Brect2(80+PlusX,230,textWidth(Dialogue[character][Event][Box].Array[Sentence])+20,89);}
  }  
  stroke(TextColor);
  line(319,230,319,318);  
  line(80,230,319,230);
  line(80,319,319,319);
  line(80,230,80,319);
  if(!(PlusX>textWidth(Dialogue[character][Event][Box].Array[Sentence])+20 && Sentence==3)){zPressed=false;}
  if(PlusX>textWidth(Dialogue[character][Event][Box].Array[Sentence]) && !(Sentence>=3)){
     Sentence+=1;
     PlusX=0;
  }
}

void Brect(double x,double y, double width, double height){
  noStroke();
  for(int color=0; color < height; color++){
    fill(round((double)(190-(100000*color/height))),round((double)(190-(145*color/height))),255);
  	rect(x,y+color,width,1);     
  }
  stroke(TextColor);
  strokeWeight(2);
  line(x,y,x+width,y);
  line(x,y+height,x+width,y+height);
  line(x,y,x,y+height);
  line(x+width,y,x+width,y+height);
}
void Brect2(double x,double y, double width, double height){
  noStroke();
  for(int color=0; color < height; color++){
    fill(round((double)(190-(100000*color/height))),round((double)(190-(145*color/height))),255);
  	rect(x,y+color,width,1);     
  }
}
