boolean FightStart = true;
int AllEnemiesNLoot;
void Encounter(Stages X, int min, int max, int SpawnRate, int MinE, int MaxE){
	if(sqrt(pow(StartX-CameraX,2)+pow(StartY-CameraY,2)) > TileSize){
    test=true; 
    StartX=CameraX;
    StartY=CameraY;
  }      
  if(test && !Fighting){
    pasos-=SpawnRate-NoBattle;
    test=false;
  }
  if(pasos < 0){    
    if(FightStart){        
      Animation.AnimatingE=true;
      Enemies = new Enemy[round(random(MinE,MaxE))];
      EnemyList = new Enemy[sizeOf(Enemies)];
      Queque = new Movement[sizeOf(Team) + sizeOf(EnemyList)];
      for(int i = 0; i<sizeOf(EnemyList); i++){
      	Enemies[i] = X.StgEnemies[round(random(0,sizeOf(X.StgEnemies)-1))];  
      }      
      for(int i = 0; i < sizeOf(Enemies); i++){
        Enemies[i].Lvl = round(random(min,max));
        EnemyList[i] = new Enemy(Enemies[i].ID,Enemies[i].width,Enemies[i].height,
                                 Enemies[i].Lvl,Enemies[i].MP,Enemies[i].HP,
                                 Enemies[i].Atk,Enemies[i].Mag,Enemies[i].Def,Enemies[i].Speed, 
                                 Enemies[i].Name,sizeOf(Enemies[i].MovementSet),
                                 sizeOf(Enemies[i].LootTable),round(random(1,Enemies[i].NLoot)));
        for(int o = 0; o < sizeOf(Enemies[i].MovementSet); o++){
          EnemyList[i].MovementSet[o] = Enemies[i].MovementSet[o];
        }
        for(int o = 0; o < sizeOf(Enemies[i].LootTable); o++){
          EnemyList[i].LootTable[o] = Enemies[i].LootTable[o];
        }
        for(int o = 0; o < Enemies[i].Lvl; o++){
          EnemyList[i].LvlUp();
        }
        AllEnemiesNLoot += EnemyList[i].NLoot;
      }  
      Animation.Animating = false;
      ArrowX = 165;
      ArrowY = 227;
      if(Team[0].HP <= 0){
        Queque[CurrentCharacter-1] = Ignore;
        Team[CurrentCharacter-1].TargetM = Team[CurrentCharacter-1];
        Team[CurrentCharacter-1].TargetE = null;
        NextCh();
      }
      Priority = new int[sizeOf(Team)+sizeOf(EnemyList)];      
			Loot = new Item[AllEnemiesNLoot];
      FightStart = false;
    }
    if(!Animation.AnimatingE){
    	Battle();    
    }
    if(Animation.AnimatingE){
    	Animation.Encounters(); 
    }
  }
}
Character Using = XavChild;
void update(){ 
  seconds = round(millis()/1000);
  minutes = round(seconds/60);
  hours = round(minutes/60);  
  if(xPressed){
    Using.speed = 5;
  }else{
    Using.speed = 3;
  }
  if(Stop || Fighting || Animation.AnimatingE){ 
    upPressed=false;
    leftPressed=false;
    rightPressed=false;
    downPressed=false;    
  }
  if(!Stop){  
    if(!Animation.AnimatingE || !Fighting){
      TileX=CameraX;		
      TileY=CameraY;
      if(!Interacting){ 
        if(upPressed==true && !leftPressed && !rightPressed && !downPressed){       
          Using.GetTest(1);
        }
        if(leftPressed==true && !rightPressed && !upPressed && !downPressed){
          Using.GetTest(2);
        }
        if(downPressed==true && !upPressed && !rightPressed && !leftPressed){
          Using.GetTest(3);     
        }
        if(rightPressed==true && !upPressed && !downPressed && !leftPressed){      
          Using.GetTest(4);     
        }   
    	}        
      UpdateStages();   
      if(Interacting){
        CanMove = false;
      }
      if(!Interacting){Using.move();}else Animation.WalkTimer=0;
        TileX=CameraX;		
        TileY=CameraY;
      }
  }  
  if(MainMenuState == -1){
    if(aPressed && ControlState >= 3){
      aPressed = false;
      MainMenuState = 0;
      ArrowY = 178;
  		ArrowX = 76;
    }
    if(downPressed && ControlState == 0){
      rightPressed = false;
      ControlState++;
    }
    if(zPressed && ControlState == 1){
      zPressed = false;
      ControlState++;
    }
    if(xPressed && ControlState == 2){
      xPressed = false;
      ControlState++;
    }
  }
  if(MainMenuState == 0){ 
    if(ArrowDown && ArrowY == 178.0){
      ArrowY = 215;
      ArrowX = 70;
    }
    if(ArrowUp && ArrowY == 215.0){
      ArrowY = 178;
      ArrowX = 76;
    }
    if(zPressed && ArrowY == 178.0){
      zPressed = false;
      MainMenuState = 1;
    }
  }
  
}

