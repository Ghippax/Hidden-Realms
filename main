  public void InputQueque(int Index){
    for(int i = 0; i < sizeOf(MovementSet); i++){
      if(MP < MovementSet[i].Cost){
        full += 1; 
      }
    }
    Movement A;
    TargetM = Team[round(random(0,sizeOf(Team)-1))];
    if(full == sizeOf(MovementSet)){
      A = Attack;    	
      full = 0;
      Queque[sizeOf(Team)+Index] = A; 
    }else{ 
      A = MovementSet[round(random(0,sizeOf(MovementSet)-1))];
      while(A.Cost > MP){
        A = MovementSet[round(random(0,sizeOf(MovementSet)-1))];
      }
      if(MP >= A.Cost){         
        MP -= A.Cost;
        full = 0;
        Queque[sizeOf(Team)+Index] = A; 
      }
    }    
