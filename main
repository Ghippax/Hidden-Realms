double Dist(double x, double y, double x2, double y2){
  return sqrt(pow(x-x2,2)+pow(y-y2,2))
}

void onKeyPressed(String tecla) {
  switch (tecla) {
    case "up":
    	upPressed = true;
    	ArrowUp = true;
    	break;
    case "left":
    	leftPressed = true;
    	ArrowLeft = true;
    	break;
    case "down":
    	downPressed = true;
    	ArrowDown = true;
    	break;
    case "right":
    	rightPressed = true;
    	ArrowRight = true;
    	break;
    case "z":
    	zPressed = true;
    	break;
    case "a":
    	aPressed = true;
    	break;
    case "x":
    	xPressed = true;
    	break;
    case "Z":
    	zPressed = true;
    	break;
    case "A":
    	aPressed = true;
    	break;
    case "X":
    	xPressed = true;
    	break;
  }
}

void onKeyReleased(String tecla) {
  switch (tecla) {
    case "up":
    	upPressed = false;
    	ArrowUp = false;
    	break;
    case "left":
    	leftPressed = false;
    	ArrowLeft = false;
    	break;
    case "down":
    	downPressed = false;
    	ArrowDown = false;
    	break;
    case "right":
    	rightPressed = false;
    	ArrowRight = false;
    	break;
    case "z":
    	zPressed = false;
    	break;
    case "a":
    	aPressed = false;
    	break;
    case "x":
    	xPressed = false;
    	break;
    case "Z":
    	zPressed = false;
    	break;
    case "A":
    	aPressed = false;
    	break;
    case "X":
    	xPressed = false;
    	break;
  }
}

class ArrayList{
  String[] Array;
  int Size;
  public ArrayList(){
  	Array = new String[null];
    Size = 0;
  }
  public void Add(String Adding){
    String aux[] = new String[Size+1];
    for(int i = 0; i < sizeOf(Array); i++){
      aux[i]=Array[i];
    }
    aux[Size] = Adding;
    Size = sizeOf(aux);
    Array = aux;
  }
  public void Remove(int Index){
    String aux[] = new String[Size-1];
    int n;
    for(int i = 0; i < sizeOf(Array); i++){
      if(i != Index){
      	aux[n]=Array[i];  
        n++;
      }     
    }
    Size = sizeOf(aux);
    Array = aux;
  }
  public void Expand(int HowMuch){
    String aux[] = new String[Size+HowMuch];
    for(int i = 0; i < sizeOf(Array); i++){
      aux[i]=Array[i];
    }
    Size=sizeOf(aux);
    Array = aux;
  }
  public void Reduce(int HowMuch){
    String aux[] = new String[Size-HowMuch];
    for(int i = 0; i < sizeOf(aux); i++){
      aux[i]=Array[i];
    }
    Size=sizeOf(aux);
    Array = aux;
  }
}
